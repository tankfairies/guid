{
  "name": "tankfairies/guid",
  "description": "GUID Generator",
  "type": "library",
  "keywords": ["guid"],
  "homepage": "https://tankfairies.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Jon",
      "email": "jon@tankfairies.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/tankfairies/guid/issues"
  },
  "prefer-stable": true,
  "require": {
    "php": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Tankfairies\\Guid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/module-phpbrowser": "^2.0",
    "codeception/module-asserts": "^2.0"
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
